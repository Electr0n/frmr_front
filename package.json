{
  "name": "frmr_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm run build-css:watch & npm start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "stylus ./src/ -o ./src/index.css",
    "build-css:watch": "stylus -w ./src/ -o ./src/index.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepush": "cross-env CI=true npm test",
    "flow": "flow"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/addons": "^3.3.12",
    "@storybook/cli": "^3.3.12",
    "@storybook/react": "^3.3.12",
    "babel-core": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.65.0",
    "jest-enzyme": "^4.2.0",
    "react-test-renderer": "^16.2.0",
    "stylus": "^0.54.5"
  }
}
